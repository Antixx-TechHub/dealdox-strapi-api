{
  "kind": "collectionType",
  "collectionName": "successstories",
  "info": {
    "singularName": "successstory",
    "pluralName": "successstories",
    "displayName": "Success Story",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "shortDesc": {
      "type": "string"
    },
    "btnText": {
      "type": "string",
      "default": "Read More"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "longDesc": {
      "type": "richtext",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "success_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::success-category.success-category",
      "mappedBy": "success_stories"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
